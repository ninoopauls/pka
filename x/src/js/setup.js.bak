                            "Unit": "m",
                            "Available": "Yes"
                        } */

                      // let newName = normalizeHeaderName(headerName);
                      headers.push(headerName);
                       

                  }else{
                      var key = headers[j];
                      if(key){
                          rowData[key] = cells[j].trim();
                          rowData[key]  = (cells[j]).replace(/"/g, "")
